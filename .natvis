<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
	<Type Name="Ck::Array&lt;*&gt;">
		<DisplayString Condition="mSize == 0 &amp;&amp; mCapacity == 0">Empty</DisplayString>
		<DisplayString Condition="mSize == 0 &amp;&amp; mCapacity &gt; 0">Empty, Capacity={mCapacity}</DisplayString>
		<DisplayString Condition="mSize &gt; 0">{{ Size={mSize} }}</DisplayString>
		<Expand>
			<Item Name="[size]" ExcludeView="simple">mSize</Item>
			<Item Name="[capacity]" ExcludeView="simple">mCapacity</Item>
			<ArrayItems>
				<Size>mSize</Size>
				<ValuePointer>($T1*)mAllocator.mData</ValuePointer>
			</ArrayItems>
		</Expand>
	</Type>
	<Type Name="Ck::String">
		<DisplayString Condition="mCharacters.mSize == 0 &amp;&amp; mCharacters.mCapacity == 0">Empty</DisplayString>
		<DisplayString Condition="mCharacters.mSize == 0 &amp;&amp; mCharacters.mCapacity &gt; 0">Empty, Capacity={mCharacters.mCapacity - 1}</DisplayString>
		<DisplayString Condition="mCharacters.mSize &gt; 0">{{ Length={mData.mSize - 1} }}</DisplayString>
		<Expand>
			<Item Name="[length]" ExcludeView="simple">mCharacters.mSize - 1</Item>
			<Item Name="[capacity]" ExcludeView="simple">mCharacters.mCapacity - 1</Item>
			<ArrayItems>
				<Size>mSize</Size>
				<ValuePointer>(wchar_t*)mCharacters.mAllocator.mData</ValuePointer>
			</ArrayItems>
		</Expand>
	</Type>
	<Type Name="Ck::StringView">
		<DisplayString Condition="mLength == 0">Empty</DisplayString>
		<DisplayString Condition="mLength &gt; 0">{{ Length={mLength} }}</DisplayString>
		<Expand>
			<Item Name="[length]" ExcludeView="simple">mLength</Item>
			<ArrayItems>
				<Size>mLength</Size>
				<ValuePointer>(wchar_t*)mString</ValuePointer>
			</ArrayItems>
		</Expand>
	</Type>
	<Type Name="Ck::Vector2&lt;*&gt;">
		<DisplayString>{mComponents[0]}, {mComponents[1]}</DisplayString>
		<Expand>
			<Item Name="x" ExcludeView="simple">mComponents[0]</Item>
			<Item Name="y" ExcludeView="simple">mComponents[1]</Item>
		</Expand>
	</Type>
	<Type Name="Ck::Vector3&lt;*&gt;">
		<DisplayString>{mComponents[0]}, {mComponents[1]}, {mComponents[2]}</DisplayString>
		<Expand>
			<Item Name="x" ExcludeView="simple">mComponents[0]</Item>
			<Item Name="y" ExcludeView="simple">mComponents[1]</Item>
			<Item Name="z" ExcludeView="simple">mComponents[2]</Item>
		</Expand>
	</Type>
	<Type Name="Ck::Vector4&lt;*&gt;">
		<DisplayString>{mComponents[0]}, {mComponents[1]}, {mComponents[2]}, {mComponents[3]}</DisplayString>
		<Expand>
			<Item Name="x" ExcludeView="simple">mComponents[0]</Item>
			<Item Name="y" ExcludeView="simple">mComponents[1]</Item>
			<Item Name="z" ExcludeView="simple">mComponents[2]</Item>
			<Item Name="w" ExcludeView="simple">mComponents[3]</Item>
		</Expand>
	</Type>
</AutoVisualizer>